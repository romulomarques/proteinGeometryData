{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "PDB: File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bp.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) sbbu_dbg.exe",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/solvers/sbbu/sbbu_dbg.exe",
            "args": [
                "-nmr", "${workspaceFolder}/dmdgp_HA9H/1slj_model1_chainA_segment2.csv",
                "-tmax", "3600",                
                "-dfs_all", "1",
                "-outdir", "${workspaceFolder}/dmdgp_HA9H_sbbu"
            ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build sbbu_dbg.exe",
            "miDebuggerPath": "/usr/bin/gdb",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "(gdb) sbbu.exe",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/solvers/sbbu/sbbu.exe",
            "args": [
                "-nmr", "${workspaceFolder}/dmdgp_HA9H/1wj5_model1_chainA_segment5.csv",
                "-tmax", "3600",
                "-dtol", "1e-5",
                "-fbs", "0",
                "-dfs_all", "1",
                "-outdir", "${workspaceFolder}/dmdgp_HA9H_sbbu"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build sbbu.exe",
            "miDebuggerPath": "/usr/bin/gdb",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}